[{"key":"SCENARIO_s40_01010000_05_0000","text":"Motion"},
{"key":"SCENARIO_s40_01010000_05_0001","text":"Motion"},
{"key":"SCENARIO_s40_01010000_05_0010","text":"......Oh......"},
{"key":"SCENARIO_s40_01010000_05_0020","text":"......Hey, a rainbow!"},
{"key":"SCENARIO_s40_01010000_05_0030","text":"A rainbow?"},
{"key":"SCENARIO_s40_01010000_05_0040","text":"Really, it’s huge......"},
{"key":"SCENARIO_s40_01010000_05_0050","text":"Wow......!"},
{"key":"SCENARIO_s40_01010000_05_0060","text":"It’s beautiful......"},
{"key":"SCENARIO_s40_01010000_05_0070","text":"Oh......"},
{"key":"SCENARIO_s40_01010000_05_0080","text":"There might be two rainbows......"},
{"key":"SCENARIO_s40_01010000_05_0090","text":"Huh?"},
{"key":"SCENARIO_s40_01010000_05_0100","text":"Inside the big rainbow, faintly......\nthere’s a smaller rainbow......I think......"},
{"key":"SCENARIO_s40_01010000_05_0110","text":"Ah......"},
{"key":"SCENARIO_s40_01010000_05_0120","text":"......!\nAmazing, amazing! It’s true!"},
{"key":"SCENARIO_s40_01010000_05_0130","text":"Sorry, can I take a photo real quick?"},
{"key":"SCENARIO_s40_01010000_05_0140","text":"Sure! Let’s switch seats!"},
{"key":"SCENARIO_s40_01010000_05_0150","text":"You\u0027re taking one too, right, Meguru?\nI’m fine from here──"},
{"key":"SCENARIO_s40_01010000_05_0160","text":"Tunnel......"},
{"key":"SCENARIO_s40_01010000_05_0170","text":"We’re out!"},
{"key":"SCENARIO_s40_01010000_05_0180","text":"Ah......"},
{"key":"SCENARIO_s40_01010000_05_0190","text":"Alright, now──"},
{"key":"SCENARIO_s40_01010000_05_0200","text":"Ah......"}]
