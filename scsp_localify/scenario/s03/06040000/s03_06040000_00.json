[{"key":"SCENARIO_s03_06040000_00_0001","text":"Motion"},
{"key":"SCENARIO_s03_06040000_00_0010","text":"Eh, are you going somewhere?"},
{"key":"SCENARIO_s03_06040000_00_0020","text":"Just to the convenience store,\nto buy some drinks."},
{"key":"SCENARIO_s03_06040000_00_0030","text":"Do you want anything?"},
{"key":"SCENARIO_s03_06040000_00_0040","text":"Oh, carbonated drinks?"},
{"key":"SCENARIO_s03_06040000_00_0004","text":"Ah, I\u0027ll take coffee...!"},
{"key":"SCENARIO_s03_06040000_00_0005","text":"I\u0027ll go with you."},
{"key":"SCENARIO_s03_06040000_00_0006","text":"I\u0027m fine."},
{"key":"SCENARIO_s03_06040000_01_0010","text":"...Is that so?"},
{"key":"SCENARIO_s03_06040000_01_0020","text":"Then please go ahead and go by yourself."},
{"key":"SCENARIO_s03_06040000_02_0010","text":".............I\u0027ll stop going."},
{"key":"SCENARIO_s03_06040000_02_0020","text":"Huh, carbonated drinks..."},
{"key":"SCENARIO_s03_06040000_03_0010","text":"...Huh?"},
{"key":"SCENARIO_s03_06040000_03_0020","text":"Can you please not interrupt the conversation?"}]
